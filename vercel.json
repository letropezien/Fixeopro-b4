{
  "version": 2,
  "routes": [
    {
      "src": "/admin",
      "dest": "/admin"
    },
    {
      "src": "/admin/(.*)",
      "dest": "/admin/$1"
    },
    {
      "src": "/categories/(.*)",
      "dest": "/categories/$1"
    },
    {
      "src": "/profil-pro/(.*)",
      "dest": "/profil-pro/$1"
    },
    {
      "src": "/demande/(.*)",
      "dest": "/demande/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}
